<h1>Edit Link<h1>
<%= form_with model: @url, url: url_path, method: :put do |f| %>
    <%= f.number_field :id, id: 'id', type: :hidden %>
    <p>
        <%= f.label 'Enter Target Url:'%><br>
        <%= f.text_field :target_url, id: 'target_url' %>
    </p>
    <p>
        <%= f.submit 'Save' %>
    </p>
<% end %>