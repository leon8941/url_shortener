<h1>Register<h1>
<%= form_with model: @user, url: registers_path do |f| %>
    <p>
        <%= f.label 'first name:'%><br>
        <%= f.text_field :first_name, id: 'first_name' %>
    </p>
        <p>
        <%= f.label 'last name:'%><br>
        <%= f.text_field :last_name, id: 'last_name' %>
    </p>
    <p>
        <%= f.label 'email:'%><br>
        <%= f.text_field :email, id: 'email' %>
    </p>
    <p>
        <%= f.label 'password:'%><br>
        <%= f.password_field :password, id: 'password' %>
    </p>
    <p>
        <%= f.submit 'Register' %>
    </p>
<% end %>