<h1>Sign in<h1>
 <%= form_with url: logins_path do |f| %>
    <p>
        <%= f.label 'email:'%><br>
        <%= f.text_field :email, id: 'email' %>
    </p>
    <p>
        <%= f.label 'password:'%><br>
        <%= f.password_field :password, id: 'password' %>
    </p>
    <p>
        <%= f.submit 'Sign in' %>
    </p>
<% end %>